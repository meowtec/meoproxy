{
  "name": "myproxy",
  "version": "0.0.0",
  "description": "",
  "main": "build/bootstrap.js",
  "scripts": {
    "i": "npm install && tsd install",
    "lint": "tslint ./src/*.ts* ./src/**/*.ts* ./src/**/**/*.ts*",
    "electron": "electron .",
    "build:back": "tsc -p . && babel build --out-dir build",
    "build:front": "webpack",
    "build": "npm run build:back && npm run build:front",
    "start": "npm run build:back && MEOP_ENV=dev_watch npm run electron",
    "start:once": "npm run build:front && npm run build:back && MEOP_ENV=dev npm run electron",
    "watch": "webpack-dev-server --port 11200",
    "test": "npm run build:back && mocha build/__tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/meowtec/meoproxy.git"
  },
  "author": "Meowtec<bertonzh@gmail.com>",
  "private": true,
  "dependencies": {
    "babel-plugin-transform-es2015-spread": "^6.6.5",
    "catro": "next",
    "electron-prebuilt": "^0.37.2",
    "ip": "^1.0.1",
    "lodash": "^3.10.1",
    "log4js": "^0.6.32",
    "mkdirp": "0.5.1",
    "react": "^0.14.7",
    "react-addons-pure-render-mixin": "^0.14.7",
    "react-dom": "^0.14.3",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-async-to-generator": "^6.7.4",
    "babel-plugin-transform-es2015-destructuring": "^6.5.0",
    "babel-plugin-transform-es2015-parameters": "^6.5.0",
    "css-loader": "^0.23.1",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.1",
    "tsd": "^0.6.5",
    "tslint": "^3.6.0",
    "typescript": "^1.8.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  }
}
